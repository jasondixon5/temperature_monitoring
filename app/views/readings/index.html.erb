<h1>Temperature Readings</h1>

<table>
<thead>
<tr>
<th>Device Location</th>
<th>Reading</th>
<th>Date</th>

</tr>
</thead>

<tbody>
<% @readings.each do |reading| %>
<tr>
<td><%= reading.device.location.name %> (device id <%= reading.device.id %>)</td>
<td><%= reading.temperature %></td>
<td><%= reading.created_at %></td>

</tr>
<% end %>
</tbody>
</table>

