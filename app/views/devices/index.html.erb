<h1>Devices</h1>

<table>
<thead>
<tr>
<th>Device ID</th>
<th>Current Device Location</th>
<th>Device Created Date</th>
</tr>
</thead>

<tbody>
<% @devices.each do |device| %>
<tr>
<td><%= device.id %></td>
<td><%= device.location.name %> (<i>Loc ID <%= device.location_id %></i>)</td>
<td><%= device.created_at %></td>
</tr>
<% end %>
</tbody>
</table>